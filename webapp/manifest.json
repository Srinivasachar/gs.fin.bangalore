{
  "_version": "1.32.0",
  "sap.app": {
    "id": "gs.fin.bangalore",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZCREATE_CDS_FROM_XML_SRV",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "gs.fin.bangalore.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.96.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "gs.fin.bangalore.i18n.i18n"
        }
      },
      "process": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data/CdsTree.json"
      },
      "Guided": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data/QuickRDCreation.json"
      },
      "": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultUpdateMethod": "PUT",
					"defaultCountMode": "Inline",
					"metadataUrlParams": {
						"sap-documentation": "heading"
					},
          "headers":{
            "x-csrf-token": "Fetch"
          }
				}
			}
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "gs.fin.bangalore.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Overview",
          "pattern": "",
          "target": ["Overview"]
        },
        {
          "name": "FirstView",
          "pattern": "FirstView",
          "target": ["FirstView"]
        },
        {
          "name": "GuidedAssistant",
          "pattern": "GuidedAssistant",
          "target": ["GuidedAssistant"]
        }
      ],
      "targets": {
        "Overview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Overview",
          "viewName": "Overview"
        },
        "FirstView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "FirstView",
          "viewName": "FirstView"
        },
        "GuidedAssistant": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "GuidedAssistant",
          "viewName": "GuidedAssistant"
        }
      }
    }
  }
}
