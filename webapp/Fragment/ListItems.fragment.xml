<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="mySelectDialog"
		noDataText="No CDS View Found"
		title="Select CDS View"
		search=".onSearch"
		confirm=".onCDSF4Confirmation"
		cancel=".onCDSF4Confirmation"
		items="{
			path: 'CDSViewsCollection>/'
		}">
		<StandardListItem
			title="{CDSViewsCollection>Cdsviewname}"
			description=""
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>